html, body, div, h1, h2, h3, h4, h5, p
  margin 0
  padding 0

body
  font-family "Helvetica Neue", Helvetica, Arial, sans-serif
  line-height 1.4
  word-break keep-all

main
  display flex
  align-items center
  justify-content center
  text-align center
  margin 6rem auto 3rem
  flex-direction column